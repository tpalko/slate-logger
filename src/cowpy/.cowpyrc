{   
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "format": "Format.DETAILED"
        },
        "exceptions": {
            "class": "logging.FileHandler",
            "filename": "cowpy_exceptions.log",
            "level": "DEBUG",
            "format": "Format.DETAILED"
        }
    },
    "loggers": {    
        "__cowpy__": {
            "handlers": ["console", "exceptions"],
            "level": "DEBUG"
        }        
    }
}